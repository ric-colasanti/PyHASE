    "import random as rnd\n",
    "\n",
    "def rgb(n):\n",
    "    d = 255\n",
    "    rgb=[]\n",
    "    for i in range(3):\n",
    "        x = n%d\n",
    "        rgb.append(int(x/d*255))\n",
    "        n = n-x\n",
    "        d=d*255\n",
    "    print(rgb)"
